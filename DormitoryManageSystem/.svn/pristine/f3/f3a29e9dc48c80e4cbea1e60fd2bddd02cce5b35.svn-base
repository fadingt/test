package org.vaalbara.bean;

/**
 * Created by Administrator on 2018/3/12.
 */
public class Dromitory {
    private int dId;//宿舍id
    private int bedIdCount;//床总数
    private int sIdCount;//学生入住总数
    private int nullBed;//剩余床数

    public Dromitory() {
    }

    public Dromitory(int dId, int bedIdCount, int sIdCount, int nullBed) {
        this.dId = dId;
        this.bedIdCount = bedIdCount;
        this.sIdCount = sIdCount;
        this.nullBed = nullBed;
    }




    public Dromitory(int dId) {
        this.dId = dId;
    }

    public int getdId() {
        return dId;
    }

    public void setdId(int dId) {
        this.dId = dId;
    }

    public int getBedIdCount() {
        return bedIdCount;
    }

    public void setBedIdCount(int bedIdCount) {
        this.bedIdCount = bedIdCount;
    }

    public int getsIdCount() {
        return sIdCount;
    }

    public void setsIdCount(int sIdCount) {
        this.sIdCount = sIdCount;
    }

    public int getNullBed() {
        return nullBed;
    }

    public void setNullBed(int nullBed) {
        this.nullBed = nullBed;
    }


    @Override
    public String toString() {
        return "Dromitory{" +
                "dId=" + dId +
                ", bedIdCount=" + bedIdCount +
                ", sIdCount=" + sIdCount +
                ", nullBed=" + nullBed +
                '}';
    }
}
