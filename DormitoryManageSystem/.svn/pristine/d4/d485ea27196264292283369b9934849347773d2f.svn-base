package org.vaalbara.bean;

/**
 * Created by Administrator on 2018/3/15.
 */
public class Admin {
    private int aId;
    private  String aName;
    private int aStage;
    private String aPass;
    private String aPassNew;

    @Override
    public String toString() {
        return "Admin{" +
                "aId=" + aId +
                ", aName='" + aName + '\'' +
                ", aStage=" + aStage +
                ", aPass='" + aPass + '\'' +
                ", aPassNew='" + aPassNew + '\'' +
                '}';
    }

    public Admin(int aId, String aName, int aStage, String aPass, String aPassNew) {
        this.aId = aId;
        this.aName = aName;
        this.aStage = aStage;
        this.aPass = aPass;
        this.aPassNew = aPassNew;
    }

    public String getaPassNew() {
        return aPassNew;
    }

    public void setaPassNew(String aPassNew) {
        this.aPassNew = aPassNew;
    }

    public int getaId() {
        return aId;
    }

    public void setaId(int aId) {
        this.aId = aId;
    }

    public String getaName() {
        return aName;
    }

    public void setaName(String aName) {
        this.aName = aName;
    }

    public int getaStage() {
        return aStage;
    }

    public void setaStage(int aStage) {
        this.aStage = aStage;
    }

    public String getaPass() {
        return aPass;
    }

    public void setaPass(String aPass) {
        this.aPass = aPass;
    }

    public Admin(int aId, String aName, int aStage, String aPass) {
        this.aId = aId;
        this.aName = aName;
        this.aStage = aStage;
        this.aPass = aPass;
    }

    public Admin() {
    }

}
