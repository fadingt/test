    function add() {
        var sId = $("#roleId").val();
        var sName = $("#roleName").val();
        var sEmail = $("#roleEmail").val();
        var sPhone = $("#rolePhone").val();
        var dId = $("#roleDid").val();
        var pId = $("#rolePid").val();
        var sClass = $("#roleClass").val();
        $.ajax({
            url:"../student/gai",
            type:"post",

            data:{
                sId:sId,
                sName:sName,
                sEmail:sEmail,
                sPhone:sPhone,
				dId:dId,
				pId:pId,
				sClass:sClass
            },
            dataType:"json",
            async:false,
            success:function (result) {
                if(result>0){
                    alert("修改成功");
                }else{
                    alert("修改失败");
                }
            }
        });
    }

