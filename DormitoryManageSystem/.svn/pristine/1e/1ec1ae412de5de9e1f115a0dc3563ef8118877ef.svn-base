	<body>
		
		<div class="am-cf admin-main">
			<!-- content start -->
			<div class="admin-content">
				<div class="admin-content-body">
					<div class="am-g">
						<form class="am-form am-form-horizontal" action="role/addroleSubmit.action" method="post" style="padding-top: 30px;">
							<input value="504" name="roleId" type="hidden">
							<div class="am-form-group">
								<label for="user-name" class="am-u-sm-3 am-form-label">
									学生编号
								</label>
								<div class="am-u-sm-9">
									<input id="roleId" required="" placeholder="aaaa" value="" name="departName" type="text">
									<!--<small id="helpRole">输入部门名称。</small>-->
								</div>
							</div>
							<div class="am-form-group">
								<label for="user-name" class="am-u-sm-3 am-form-label">
									学生姓名
								</label>
								<div class="am-u-sm-9">
									<input id="roleName" required="" placeholder="请输入学生姓名" value="" name="departName" type="text">
									<!--<small id="helpRole">输入部门名称。</small>-->
								</div>
							</div>
							<div class="am-form-group">
								<label for="user-name" class="am-u-sm-3 am-form-label">
									学生邮箱
								</label>
								<div class="am-u-sm-9">
									<input id="roleEmail" required="" placeholder="请输入学生邮箱" value="" name="departName" type="text">
									<!--<small id="helpRole">输入部门名称。</small>-->
								</div>
							</div>
							<div class="am-form-group">
								<label for="user-name" class="am-u-sm-3 am-form-label">
									学生电话
								</label>
								<div class="am-u-sm-9">
									<input id="rolePhone" required="" placeholder="请输入学生电话" value="" name="departName" type="text">
									<!--<small id="helpRole">输入部门名称。</small>-->
								</div>
							</div>
							<div class="am-form-group">
								<label for="user-name" class="am-u-sm-3 am-form-label">
									寝室编号
								</label>
								<div class="am-u-sm-9">
									<input id="roleDid" required="" placeholder="请输入寝室编号" value="" name="departName" type="text">
									<!--<small id="helpRole">输入部门名称。</small>-->
								</div>
							</div>
							<div class="am-form-group">
								<label for="user-name" class="am-u-sm-3 am-form-label">
									专业编号
								</label>
								<div class="am-u-sm-9">
									<input id="rolePid" required="" placeholder="请输入专业编号" value="" name="departName" type="text">
									<!--<small id="helpRole">输入部门名称。</small>-->
								</div>
							</div>
							<div class="am-form-group">
								<label for="user-name" class="am-u-sm-3 am-form-label">
									学生班级
								</label>
								<div class="am-u-sm-9">
									<input id="roleClass" required="" placeholder="请输入学生班级" value="" name="departName" type="text">
									<!--<small id="helpRole">输入部门名称。</small>-->
								</div>
							</div>

							<div class="am-form-group">
								<div class="am-u-sm-9 am-u-sm-push-3">
									<input id="addRole" class="am-btn am-btn-success" value="提交" type="button" onclick="add()">
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</body>
